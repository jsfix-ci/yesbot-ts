{
  "name": "tsbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "prettier": "@yestheoryfam/prettier-config",
  "scripts": {
    "build:docker": "docker build --force-rm --label yesbot --build-arg VCS_REF=$(git rev-parse --short HEAD) -t yesbot .",
    "build:prepare": "NODE_ENV=production && rm -rf node_modules && yarn install && yarn run tsc",
    "build:run": "docker run --link yesbot-ts_postgres_1 .",
    "codegen": "graphql-codegen --config codegen.yml",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate",
    "lint": "prettier --check \"src/**/*.(js|ts|json)\" \"__tests__/**/*.(js|ts|json)\"",
    "lint:fix": "prettier --write \"src/**/*.(js|ts|json)\" \"__tests__/**/*.(js|ts|json)\"",
    "postinstall": "yarn run db:generate",
    "start": "env-cmd ts-node-dev --files --respawn src/index.ts",
    "start:prod": "node build/src/index.js",
    "start:prodOld": "env-cmd ts-node src/index.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "tsc": "tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "1.0.1",
    "@prisma/client": "4.2.1",
    "axios": "0.27.2",
    "countries-and-timezones": "3.3.0",
    "country-code-emoji": "2.3.0",
    "date-fns": "2.29.1",
    "date-fns-tz": "1.3.6",
    "discord-api-types": "0.37.2",
    "discord.js": "14.2.0",
    "glob": "8.0.3",
    "graphql": "^15.8.0",
    "graphql-request": "^4.3.0",
    "node-cron": "3.0.1",
    "path": "0.12.7",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "ts-node": "10.9.1",
    "uuid": "8.3.2",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@atmina/local-typescript-operations": "^0.6.0",
    "@atmina/only-enum-types": "^0.5.0",
    "@graphql-codegen/cli": "^2.8.0",
    "@graphql-codegen/near-operation-file-preset": "^2.3.1",
    "@graphql-codegen/typescript-graphql-request": "^4.5.1",
    "@types/countries-and-timezones": "3.2.2",
    "@types/glob": "7.2.0",
    "@types/jest": "28.1.6",
    "@types/node": "16.11.48",
    "@types/node-cron": "^3.0.0",
    "@types/ws": "8.5.3",
    "@yestheoryfam/prettier-config": "1.0.0",
    "env-cmd": "10.1.0",
    "jest": "28.1.3",
    "jest-mock-extended": "2.0.7",
    "prettier": "2.7.1",
    "prisma": "4.2.1",
    "ts-jest": "28.0.7",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.4"
  }
}
